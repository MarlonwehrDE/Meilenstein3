        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Meilenstein3.GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
            <Button Content = "Einkaufsliste" Margin="10" Click="Einkaufsliste_Click" />
            <Button Content = "Finanzmanager" Margin = "10" Click="Finanzmanager_Click"/>
            
        </StackPanel>
        
        <Frame x:Name="MainFrame" Margin="0,50,0,0"/>

    </Grid>
</Window>
